<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>Login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CvUploadServlet</servlet-name>
        <servlet-class>servletPack.CvUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadFileServlet</servlet-name>
        <servlet-class>servletPack.DownloadFileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadAsZip</servlet-name>
        <servlet-class>servletPack.DownloadAsZip</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadZip</servlet-name>
        <servlet-class>servletPack.DownloadZip</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CvUploadServlet</servlet-name>
        <url-pattern>/CvUploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadFileServlet</servlet-name>
        <url-pattern>/DownloadFileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadAsZip</servlet-name>
        <url-pattern>/DownloadAsZip</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadZip</servlet-name>
        <url-pattern>/DownloadZip</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
